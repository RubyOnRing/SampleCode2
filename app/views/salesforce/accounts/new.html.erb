<div id="salesforce-new-account" class="card">
  <div class="card-header">
    Salesforce new Account
  </div>

  <div class="card-body">
    <div class="col-sm-12">
      <div class="form-group">
        <label for="account_name">Account Name</label>
        <input class="form-control" id="account_name" type="text" placeholder="Enter Account name">
      </div>

      <div class="form-group">
        <label for="phone">Phone</label>
        <input class="form-control" id="phone" type="text" placeholder="Enter Phone">
      </div>

      <div class="form-group">
        <label for="billing_street">Billing Street</label>
        <input class="form-control" id="billing_street" type="text" placeholder="Enter Billing Street">
      </div>

      <div class="form-group">
        <label for="billing_city">Billing City</label>
        <input class="form-control" id="billing_city" type="text" placeholder="Enter Billing City">
      </div>

      <div class="form-group">
        <label for="billing_state">Billing State</label>
        <input class="form-control" id="billing_state" type="text" placeholder="Enter Billing State">
      </div>

      <div class="form-group">
        <label for="billing_post_code">Billing Post Code</label>
        <input class="form-control" id="billing_post_code" type="text" placeholder="Enter Billing Post Code">
      </div>

      <div class="form-group">
        <label for="billing_country">Billing Country</label>
        <input class="form-control" id="billing_country" type="text" placeholder="Enter Billing Country">
      </div>

      <div class="form-group">
        <button class="btn btn-primary js-submit-new-salesforce-account">Submit</button>
      </div>
    </div>
  </div>
</div>
